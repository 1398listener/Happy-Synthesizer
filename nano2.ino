int buzzer = 13;

int pin_do = 2;
int pin_re = 3;
int pin_mi = 4;
int pin_fa = 5;
int pin_sol = 6;
int pin_la = 7;
int pin_si = 8;
int pin_sig = 9;

char rev = 'p';

void DRAW();

void setup() 
{
  Serial.begin(38400);
  
  pinMode(buzzer, OUTPUT);
  
  pinMode(pin_do, INPUT);
  pinMode(pin_re, INPUT);
  pinMode(pin_mi, INPUT);
  pinMode(pin_fa, INPUT);
  pinMode(pin_sol, INPUT);
  pinMode(pin_la, INPUT);
  pinMode(pin_si, INPUT);
  pinMode(pin_sig, INPUT);

  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C))
  {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
}

void loop() 
{
  if(Serial.available())
  {
    rev = Serial.read();
  }
  DRAW(rev);
}

void DRAW(char rev) 
{
  switch(rev)
  {
    case 'a':
    {
      display.clearDisplay();
      display.drawBitmap(0, 0, bmp_Ievan_Polkka, 120, 64, 1);
      display.display();
      break;
    }
    case 'b':
    {
      display.clearDisplay();
      display.drawBitmap(0, 0, bmp_Castle_in_the_Sky, 120, 64, 1);
      display.display();
      break;
    }
    case 'c':
    {
      display.clearDisplay();
      display.drawBitmap(0, 0, bmp_Bad_Apple, 120, 64, 1);
      display.display();
      break;
    }
    case 'x':
    {
      display.clearDisplay();
      display.drawBitmap(0, 0, bmp_record, 120, 64, 1);
      display.display();
      break;
    }
    case 'y':
    {
      display.clearDisplay();
      display.drawBitmap(0, 0, bmp_display, 120, 64, 1);
      display.display();
      break;
    }
    case 'p':
    {
      display.clearDisplay();
      display.drawBitmap((display.width() - LOGO_WIDTH ) / 2, (display.height() - LOGO_HEIGHT) / 2, bmp_logo, LOGO_WIDTH, LOGO_HEIGHT, 1);
      display.display();
      break;
    }
    default:
    {
      display.clearDisplay();
      display.drawBitmap(0, 0, bmp_happy, 120, 64, 1);
      display.display();
      break;
    }
  }
